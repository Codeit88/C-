#include<iostream>
using namespace std;
int main()
{
  int n,flag=0;
  cout<<"Enter your num: ";
  cin>>n;
  for(int i=1;i<n;i++)
  {
  	if((i*i)==n)
  	{
	  flag=1;
	  break;
    }
  	else
  	   flag=0;
  }
  if(flag==1)
  cout<<n<<" is perfect square\n";
  else
  cout<<n<<" is not perfect square\n"; 
 return 0;
}

